{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react"
  },
  "include": [
    "./.js",
    "./.jsx",
    "./.ts",
    "./.tsx",
    "./**/*.js",
    "./**/*.json",
    "./**/*.jsx",
    "./**/*.ts",
    "./**/*.tsx"
  ],
  "exclude": [
    ".build",
    ".cache",
    ".ignore",
    ".reports",
    "3rdparty",
    "covid19_scenarios_data",
    "cypress",
    "node_modules",
    "static",
    "tsconfig.json"
  ]
}
